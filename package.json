  "main": "lib/oreo-ui.common.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "demo-build": "node build/example/build.js",
    "lint": "eslint --ext .js,.vue src",
    "start": "webpack-dev-server --inline --progress --disable-host-check --host 0.0.0.0 --config ./build/webpack.demo.conf.js",
    "build:utils": "cross-env BABEL_ENV=utils babel components/utils --out-dir lib/utils ",
    "build:mixins": "cross-env BABEL_ENV=utils babel components/mixins --out-dir lib/mixins ",
    "build:locale": "cross-env BABEL_ENV=utils babel components/locale --out-dir lib/locale ",
    "build:theme": "gulp build --gulpfile components/theme-oreo/gulpfile.js && cp-cli components/theme-oreo/lib lib/theme-oreo",
    "build": "webpack  --config ./build/webpack.umd.conf.js && webpack --config ./build/webpack.common.conf.js && webpack --config ./build/webpack.components.conf.js && npm run build:utils && npm run build:mixins && npm run build:locale && npm run build:theme"
  },
